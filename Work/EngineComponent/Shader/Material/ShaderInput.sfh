struct VSInput
{
    float3 _position : POSITION;
    uint _normalTangent : NORMAL_TANGENT;
    uint _texcoord : TEXCOORD;
    uint _instanceIndex : INSTANCE_INDEX;
};

struct VertexOut
{
    float4 _position : SV_Position;
    nointerpolation uint _triangleOffset : TRIANGLE_OFFSET;
    nointerpolation uint _meshInstanceIndex : MESH_INSTANCE_INDEX;
    nointerpolation uint _materialIndex : MATERIAL_INDEX;
};